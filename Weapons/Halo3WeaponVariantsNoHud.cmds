echo Installing Halo 3 Weapons

echo RUN AFTER PORTING WEAPONS

echo Installing Unarmed Energy Sword Handle
edittag objects\weapons\melee\energy_blade\unarmed.weap
setfield defaultmodelvariant noblade
setfield firstperson[0].firstpersonmodel objects\weapons\melee\energy_blade\fp_energy_blade\fp_energy_blade.mode
